--EMPRESA G
--select * from OPCH where OPCH.DocEntry=24839
--select * from PCH1 where PCH1.DocEntry=24839

--EMPRESA I
SELECT 
OJDT.TransId, 
OJDT.RefDate,
DATEPART(YEAR, OJDT.RefDate) AS Año,
DATEPART(MONTH, OJDT.RefDate) AS Mes,
DATEPART(WEEK, OJDT.RefDate) AS Semana,
OJDT.Memo,
OJDT.LocTotal,
ISNULL (OJDT.Project,'NOMINA')AS Proyecto,
ISNULL (OPRJ.PrjName,'NOMINA') AS ProyectoNombre
FROM OJDT
LEFT OUTER JOIN OPRJ ON OJDT.Project=OPRJ.PrjCode
WHERE 
	OJDT.Memo NOT LIKE '%RETIRO%' 
AND OJDT.Memo LIKE '%NÓMINA%' OR OJDT.Memo LIKE '%NOMINA%'OR OJDT.Memo LIKE '%PAGO DE NÓMINA%' OR OJDT.Memo LIKE '%PAGO DE NOMINA%' OR OJDT.Memo LIKE '%PAGO NÓMINA%' OR OJDT.Memo LIKE '%PAGO NOMINA%'
